@model Sis_RosangelaCunha.Models.AvaliacaoContrato
@{
    ViewBag.Title = "Editar";
}
@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <h2>Editar</h2>
        <hr />
        <div class="=form-group">
            @Html.LabelFor(model => model.Comentario, new { @clas = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.Comentario, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Contato, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.Contato, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Features, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.Features, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Treinamento, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.Treinamento, new { @class = "form-control" })
            </div>
        </div>
        <input type="submit" value="Salvar" class="btn btn-sm btn-success">
    </div>

}



