@model IEnumerable<Sis_RosangelaCunha.Models.Cliente>
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="listaclientes">
    <thead>
        <tr>
            <th>
                Nome Fantasia:
            </th>
            <th>
                Razao Social:
            </th>
            <th>
                Cnpj:
            </th>
            <th>
                Cidade:
            </th>
            <th>
                Estado:
            </th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NomeFantasia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RazaoSocial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cnpj)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cidade)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EnumEstado)
                </td>
  
                <td>
                    <a href="#" onclick="EditarCliente(@item.ClienteId);" class="btn btn-info"><i class="glyphicon glyphicon-edit"></i></a>
                    <a href="#" onclick="ExcluirCliente(@item.ClienteId);" class="btn btn-danger"><i class="glyphicon glyphicon-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>
